                    android:gravity="center"
                    android:textSize="20sp"
                    android:textColor="@android:color/black"
                    android:padding="16dp"
                    android:background="@drawable/table_border" />
                <TextView
                    android:id="@+id/nivel1avatar5"
                    android:layout_width="0dp"
